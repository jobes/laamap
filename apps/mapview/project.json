{
  "name": "mapview",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/mapview/src",
  "prefix": "laamap",
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/mapview",
        "index": "apps/mapview/src/index.html",
        "main": "apps/mapview/src/main.ts",
        "polyfills": "apps/mapview/src/polyfills.ts",
        "tsConfig": "apps/mapview/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/mapview/src/assets",
          "apps/mapview/src/manifest.webmanifest"
        ],
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
          "./node_modules/lightgallery/scss/lightgallery.scss",
          "./node_modules/lightgallery/scss/lg-zoom.scss",
          "apps/mapview/src/styles.scss"
        ],
        "stylePreprocessorOptions": {
          "includePaths": ["node_modules"]
        },
        "customWebpackConfig": {
          "path": "apps/mapview/webpack.config.js"
        },
        "scripts": [],
        "allowedCommonJsDependencies": [
          "turf-jsts",
          "density-clustering",
          "skmeans",
          "rbush",
          "concaveman",
          "polygon-clipping",
          "object-assign",
          "geojson-rbush",
          "deep-equal",
          "quill",
          "events",
          "immediate",
          "spark-md5",
          "vuvuzela",
          "emulate-tab",
          "maplibre-gl",
          "@messageformat/core",
          "flat",
          "geojson-equality",
          "earcut",
          "deepmerge"
        ],
        "serviceWorker": true,
        "ngswConfigPath": "apps/mapview/ngsw-config.json"
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "1.5mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/mapview/src/environments/environment.ts",
              "with": "apps/mapview/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "cypress": {
          "fileReplacements": [
            {
              "replace": "apps/mapview/src/environments/environment.ts",
              "with": "apps/mapview/src/environments/environment.cypress.ts"
            }
          ]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:webpack-dev-server",
      "configurations": {
        "production": {
          "buildTarget": "mapview:build:production"
        },
        "development": {
          "disableHostCheck": true,
          "host": "0.0.0.0",
          "proxyConfig": "apps/mapview/src/proxy.conf.json",
          "buildTarget": "mapview:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "mapview:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/mapview/**/*.ts", "apps/mapview/**/*.html"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/mapview"],
      "options": {
        "jestConfig": "apps/mapview/jest.config.ts"
      }
    },
    "deploy": {
      "executor": "angular-cli-ghpages:deploy",
      "options": {
        "baseHref": "/laamap/",
        "name": "Viktor JÃ³ba",
        "email": "jobes666@gmail.com",
        "cname": "",
        "no-silent": true
      }
    },
    "stylelint": {
      "executor": "nx-stylelint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/mapview/**/*.css", "apps/mapview/**/*.scss"]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "mapview:build"
      }
    },
    "component-test": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/mapview/cypress.config.ts",
        "testingType": "component",
        "skipServe": true,
        "devServerTarget": "mapview:build:cypress",
        "watch": false
      },
      "configurations": {
        "production": {
          "watch": false
        },
        "development": {
          "watch": true
        }
      }
    }
  },
  "tags": []
}
