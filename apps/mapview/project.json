{
  "name": "mapview",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/mapview/src",
  "prefix": "laamap",
  "targets": {
    "real_build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/mapview",
        "index": "apps/mapview/src/index.html",
        "browser": "apps/mapview/src/main.ts",
        "polyfills": ["zone.js", "apps/mapview/src/pounchdb-fix.ts"],
        "tsConfig": "apps/mapview/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/mapview/src/assets",
          "apps/mapview/src/manifest.webmanifest"
        ],
        "styles": [
          "./node_modules/maplibre-gl/dist/maplibre-gl.css",
          "./node_modules/quill/dist/quill.snow.css",
          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
          "./node_modules/lightgallery/scss/lightgallery.scss",
          "./node_modules/lightgallery/scss/lg-zoom.scss",
          "apps/mapview/src/styles.scss"
        ],
        "stylePreprocessorOptions": {
          "includePaths": ["node_modules"]
        },
        "scripts": [],
        "allowedCommonJsDependencies": [
          "turf-jsts",
          "density-clustering",
          "skmeans",
          "rbush",
          "concaveman",
          "polygon-clipping",
          "object-assign",
          "geojson-rbush",
          "deep-equal",
          "quill",
          "events",
          "immediate",
          "spark-md5",
          "vuvuzela",
          "emulate-tab",
          "maplibre-gl",
          "@messageformat/core",
          "flat",
          "geojson-equality",
          "earcut",
          "deepmerge",
          "logrocket"
        ],
        "serviceWorker": "apps/mapview/ngsw-config.json"
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "1.5mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/mapview/src/environments/environment.ts",
              "with": "apps/mapview/src/environments/environment.processed.ts"
            }
          ],
          "outputHashing": "all"
        },
        "cypress": {
          "fileReplacements": [
            {
              "replace": "apps/mapview/src/environments/environment.ts",
              "with": "apps/mapview/src/environments/environment.cypress.ts"
            }
          ]
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "fileReplacements": [
            {
              "replace": "apps/mapview/src/environments/environment.ts",
              "with": "apps/mapview/src/environments/environment.processed.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "real_serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "mapview:real_build:production"
        },
        "development": {
          "buildTarget": "mapview:real_build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/mapview/**/*.ts", "apps/mapview/**/*.html"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/mapview"],
      "options": {
        "jestConfig": "apps/mapview/jest.config.ts"
      }
    },
    "stylelint": {
      "executor": "nx-stylelint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/mapview/**/*.css", "apps/mapview/**/*.scss"]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "mapview:build"
      }
    },
    "component-test": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/mapview/cypress.config.ts",
        "testingType": "component",
        "skipServe": true,
        "devServerTarget": "mapview:build:cypress",
        "watch": false
      },
      "configurations": {
        "production": {
          "watch": false
        },
        "development": {
          "watch": true
        }
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ftp-deploy --server `printenv FTP_SERVER` --username `printenv FTP_USERNAME` --local-dir ./dist/apps/mapview/ --server-dir subdoms/map/ --dangerous-clean-slate true --password `printenv FTP_PASSWORD`"
      }
    },
    "preprocess_env": {
      "executor": "nx:run-commands",
      "options": {
        "command": "envsubst < apps/mapview/src/environments/environment.ts > apps/mapview/src/environments/environment.processed.ts"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": []
      },
      "configurations": {
        "production": {
          "commands": [
            "nx run mapview:preprocess_env",
            "nx run mapview:real_build:production"
          ]
        },
        "development": {
          "commands": [
            "nx run mapview:preprocess_env",
            "nx run mapview:real_build:development"
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": []
      },
      "configurations": {
        "production": {
          "commands": [
            "nx run mapview:preprocess_env",
            "nx run mapview:real_serve:production"
          ]
        },
        "development": {
          "commands": [
            "nx run mapview:preprocess_env",
            "nx run mapview:real_serve:development"
          ]
        }
      },
      "defaultConfiguration": "development"
    }
  },
  "tags": []
}
